# Example environment file for the Agent Service Toolkit
# Copy this to .env and fill in your actual values

# Required: LLM Provider API Keys (at least one)
OPENAI_API_KEY=sk-your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GOOGLE_API_KEY=your-google-api-key-here

# Optional: Additional LLM Providers
DEEPSEEK_API_KEY=your-deepseek-api-key-here
GROQ_API_KEY=your-groq-api-key-here

# Optional: Ollama (for local models)
OLLAMA_MODEL=llama3.2
OLLAMA_BASE_URL=http://localhost:11434

# Optional: OpenAI Compatible API
COMPATIBLE_MODEL=your-model-name
COMPATIBLE_API_KEY=your-api-key
COMPATIBLE_BASE_URL=https://api.your-provider.com/v1

# Optional: AWS Bedrock
USE_AWS_BEDROCK=false

# Optional: Weather API
OPENWEATHERMAP_API_KEY=your-weather-api-key

# Optional: LangSmith Tracing
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=default
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your-langsmith-api-key

# Optional: Authentication
AUTH_SECRET=your-secret-auth-token

# Database Configuration
DATABASE_TYPE=sqlite  # Options: sqlite, postgres, mongodb
SQLITE_DB_PATH=checkpoints.db

# PostgreSQL Configuration (if using postgres)
POSTGRES_USER=your-postgres-user
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=your-database-name

# MongoDB Configuration (if using mongodb)
MONGO_HOST=your-mongo-host
MONGO_PORT=27017
MONGO_DB=your-mongo-database

# Todoist OAuth Configuration (Get from https://developer.todoist.com/appconsole.html)
TODOIST_CLIENT_ID=your-todoist-client-id
TODOIST_CLIENT_SECRET=your-todoist-client-secret
TODOIST_REDIRECT_URI=http://localhost:8080/api/oauth/callback/todoist

# Google Calendar OAuth Configuration (Get from https://console.developers.google.com/)
GOOGLE_CALENDAR_CLIENT_ID=your-google-client-id
GOOGLE_CALENDAR_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALENDAR_REDIRECT_URI=http://localhost:8080/api/oauth/callback/google

# Optional: GitHub Token (for repo access)
GITHUB_TOKEN=your-github-token

# Optional: Obsidian Notes Integration
OBSIDIAN_NOTES_PATH=notes
OBSIDIAN_REPO_URL=https://github.com/your-username/obsidian.git

# Optional: Vector Database Settings
VECTOR_DB_TYPE=chromadb
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
INDEX_TRACKER_SQLITE_DB=index_tracker.sqlite3
